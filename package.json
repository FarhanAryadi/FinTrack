{
  "name": "financetrackerapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "server:dev": "ts-node-dev --respawn --transpile-only server/src/index.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:start": "node dist/server/src/index.js",
    "dev": "concurrently --names \"SERVER,EXPO\" --prefix-colors \"blue.bold,green.bold\" --kill-others \"npm run server:dev\" \"npm start\"",
    "prisma:seed": "ts-node prisma/seed.ts",
    "seed": "ts-node server/src/seed.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@prisma/client": "^5.10.2",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@types/react-native-vector-icons": "^6.4.18",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "expo": "~50.0.7",
    "expo-dev-client": "~3.3.12",
    "expo-sqlite": "~13.4.0",
    "expo-status-bar": "~1.11.1",
    "express": "^4.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.2",
    "metro-react-native-babel-transformer": "^0.77.0",
    "prisma": "^5.10.2",
    "react-native-dotenv": "^3.4.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
