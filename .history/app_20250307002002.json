{
	"expo": {
		"name": "Finance Tracker",
		"slug": "financetracker",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#5B37B7"
		},
		"assetBundlePatterns": ["**/*"],
